{
  "name": "genies-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "make run-local",
    "build": "next build",
    "start": "next start",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx,.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:all": "npm run lint -- src/",
    "lint:fix:all": "npm run lint:fix -- src/",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "consumer-schema:download": "npx apollo service:download --endpoint=https://api.warehouse.dev.genies.com/consumer/graphql src/lib/apollo/consumer_schema.json",
    "consumer-schema:types": "npx apollo codegen:generate src/edge/__generated/types/consumer --localSchemaFile=src/lib/apollo/consumer_schema.json --target=typescript --tagName=gql --includes=src/edge/gql/consumer/* --outputFlat",
    "admin-schema:download": "npx apollo service:download --header \"authorization: Bearer $token\" --endpoint=https://api.warehouse.dev.genies.com/admin/graphql src/lib/apollo/admin_schema.json",
    "admin-schema:types": "npx apollo codegen:generate src/edge/__generated/types/admin --localSchemaFile=src/lib/apollo/admin_schema.json --target=typescript --tagName=gql --includes=src/edge/gql/admin/* --outputFlat"
  },
  "dependencies": {
    "@apollo/client": "3.7.4",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/link-error": "2.0.0-beta.3",
    "@aws-sdk/client-s3": "3.529.1",
    "@aws-sdk/s3-request-presigner": "3.908.0",
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@datadog/browser-logs": "4.34.3",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@tanstack/react-table": "8.7.4",
    "axios": "1.7.4",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chakra-react-select": "5.0.1",
    "cookie": "1.0.2",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "decompress": "4.2.1",
    "deepmerge": "4.2.2",
    "dotenv": "10.0.0",
    "eslint-webpack-plugin": "2.5.4",
    "framer-motion": "^6.3.11",
    "graphql": "15.8.0",
    "isomorphic-fetch": "3.0.0",
    "js-cookie": "3.0.5",
    "jwt-decode": "4.0.0",
    "libphonenumber-js": "1.10.21",
    "namor": "2.0.3",
    "next": "14.2.32",
    "papaparse": "5.4.1",
    "pino": "6.14.0",
    "pino-pretty": "4.8.0",
    "qs": "6.10.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-full-screen": "1.1.1",
    "react-hook-form": "7.43.5",
    "react-icons": "4.12.0",
    "react-select": "5.7.0",
    "statsig-react": "1.27.3",
    "uuid": "8.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": "npm run lint:fix --",
    "*.{ts,tsx,js,css,md}": "prettier --write"
  },
  "devDependencies": {
    "@playwright/test": "1.32.1",
    "@types/decompress": "4.2.4",
    "@types/node": "17.0.41",
    "@types/qs": "6.9.7",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-table": "7.7.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "apollo": "2.33.4",
    "eslint": "8.17.0",
    "eslint-config-next": "13.5.7",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jam3": "0.2.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "prettier": "2.7.1",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": "^18.x",
    "npm": "^9.x"
  }
}
